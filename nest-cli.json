{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/blogging-platform/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/blogging-platform/tsconfig.app.json"
  },
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "blogging-platform": {
      "type": "application",
      "root": "apps/blogging-platform",
      "entryFile": "main",
      "sourceRoot": "apps/blogging-platform/src",
      "compilerOptions": {
        "tsConfigPath": "apps/blogging-platform/tsconfig.app.json"
      }
    },
    "common-dtos": {
      "type": "library",
      "root": "libs/common-dtos",
      "entryFile": "index",
      "sourceRoot": "libs/common-dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-dtos/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/blogging-platform"
}